										<div class="main-slider-three__content"></div>
									</div>
								</div>
							</div>
						</div>
						<div class="swiper-slide">
							<div class="image-layer-three" style="background-image: url(assets/images/backgrounds/carousel7.jpg);height: 750px;"></div>
							<!-- /.image-layer -->

							<div class="container">
								<div class="row">
									<div class="col-xl-12">
										<div class="main-slider-three__content"></div>
									</div>
								</div>
							</div>
						</div>
						<div class="swiper-slide">

{% block body %}
	<!DOCTYPE html>
	<html lang="fr">


		<body class="">


			<div class="preloader">
				<div class="preloader__image"></div>
			</div>
			<!-- /.preloader -->


			<div class="stricky-header stricked-menu main-menu main-menu-three">
